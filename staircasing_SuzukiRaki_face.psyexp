        <Param val="# 正誤判定（例：y＝加工、n＝非加工）&amp;#10;if (key_resp.keys == 'y' and is_target_blended) or (key_resp.keys == 'n' and not is_target_blended):&amp;#10;    correct_counter += 1&amp;#10;    incorrect_counter = 0&amp;#10;    iscorrect = True&amp;#10;else:&amp;#10;    incorrect_counter += 1&amp;#10;    correct_counter = 0&amp;#10;    iscorrect = False&amp;#10;&amp;#10;# ステアケース処理&amp;#10;if correct_counter &gt;= 3:&amp;#10;    step_index = min(step_index, len(step_sizes) - 1)&amp;#10;    current_blend = max(1, current_blend - step_sizes[step_index])&amp;#10;    correct_counter = 0&amp;#10;    step_index = step_index + 1&amp;#10;elif incorrect_counter &gt;= 1:&amp;#10;    step_index = min(step_index, len(step_sizes) - 1)&amp;#10;    current_blend = min(100, current_blend + step_sizes[step_index])&amp;#10;    incorrect_counter = 0&amp;#10;    step_index = step_index + 1&amp;#10;&amp;#10;# 結果表示用（オプション）&amp;#10;text_display = ('正解' if iscorrect else '不正解')&amp;#10;" valType="extendedCode" updates="constant" name="End Routine"/>

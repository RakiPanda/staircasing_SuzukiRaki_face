        <Param val="# ランダムに画像タイプを選択（50%で加工 or 非加工）&amp;#10;is_target_blended = (randint(0, 2) == 1)&amp;#10;&amp;#10;# blend値を計算&amp;#10;if is_target_blended:&amp;#10;    blend_ratio = current_blend&amp;#10;    correctAns = 'y'&amp;#10;else:&amp;#10;    blend_ratio = 0&amp;#10;    correctAns = 'n'&amp;#10;    &amp;#10;# ファイル名を手動でゼロ埋めして作成（Auto→JS対応）&amp;#10;blend_str = &quot;%03d&quot; % blend_ratio&amp;#10;image_file = &quot;face_suzuki_smooth_faces/face_suzuki_smooth_&quot; + blend_str + &quot;_face.png&quot;&amp;#10;&amp;#10;# 出力csvファイルへの保存&amp;#10;thisExp.addData(&quot;correctAns&quot;, correctAns)&amp;#10;thisExp.addData(&quot;current_blend&quot;, current_blend)&amp;#10;thisExp.addData(&quot;blend_ratio&quot;, blend_ratio)&amp;#10;thisExp.addData(&quot;image_file&quot;, image_file)" valType="extendedCode" updates="constant" name="Begin Routine"/>
        <Param val="Auto-&gt;JS" valType="str" updates="None" name="Code Type"/>
